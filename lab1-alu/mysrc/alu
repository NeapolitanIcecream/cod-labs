#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5641d3719e90 .scope module, "alu_testbench" "alu_testbench" 2 1;
 .timescale 0 0;
v0x5641d373aa20_0 .var "a", 31 0;
v0x5641d373ab00_0 .var "b", 31 0;
v0x5641d373abd0_0 .var "op", 4 0;
v0x5641d373acd0_0 .net "out", 31 0, v0x5641d373a890_0;  1 drivers
S_0x5641d371a010 .scope module, "myalu" "alu" 2 10, 3 1 0, S_0x5641d3719e90;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "alu_a"
    .port_info 1 /INPUT 32 "alu_b"
    .port_info 2 /INPUT 5 "alu_op"
    .port_info 3 /OUTPUT 32 "alu_out"
P_0x5641d371a1e0 .param/l "A_ADD" 0 3 8, C4<00001>;
P_0x5641d371a220 .param/l "A_AND" 0 3 10, C4<00011>;
P_0x5641d371a260 .param/l "A_NOP" 0 3 7, C4<00000>;
P_0x5641d371a2a0 .param/l "A_NOR" 0 3 13, C4<00110>;
P_0x5641d371a2e0 .param/l "A_OR" 0 3 11, C4<00100>;
P_0x5641d371a320 .param/l "A_SUB" 0 3 9, C4<00010>;
P_0x5641d371a360 .param/l "A_XOR" 0 3 12, C4<00101>;
v0x5641d371a5e0_0 .net/s "alu_a", 31 0, v0x5641d373aa20_0;  1 drivers
v0x5641d373a6c0_0 .net/s "alu_b", 31 0, v0x5641d373ab00_0;  1 drivers
v0x5641d373a7a0_0 .net "alu_op", 4 0, v0x5641d373abd0_0;  1 drivers
v0x5641d373a890_0 .var "alu_out", 31 0;
E_0x5641d3714070 .event edge, v0x5641d373a7a0_0, v0x5641d371a5e0_0, v0x5641d373a6c0_0;
    .scope S_0x5641d371a010;
T_0 ;
    %wait E_0x5641d3714070;
    %load/vec4 v0x5641d373a7a0_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 5;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 5;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 5;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 5;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 5;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 5;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.0 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %add;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.1 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %sub;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.2 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %and;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.3 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %or;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.4 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %xor;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.5 ;
    %load/vec4 v0x5641d371a5e0_0;
    %load/vec4 v0x5641d373a6c0_0;
    %xnor;
    %store/vec4 v0x5641d373a890_0, 0, 32;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5641d3719e90;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "testbench.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x5641d373aa20_0, 0, 32;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x5641d373ab00_0, 0, 32;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 3, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 4, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 5, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 6, 0, 5;
    %store/vec4 v0x5641d373abd0_0, 0, 5;
    %delay 100, 0;
    %end;
    .thread T_1;
